
	<div class="container">
	  		

   		<div class="row" style="border-bottom: .3px solid gray"  id='cycle-stored-materials-headers'>
   			<div class="col-md-2">
   				<div class="cycle-stored-material-head"><strong>description</strong></div>
   			</div>	
   			<div class="col-md-2">
   				<div class="cycle-stored-material-head"><strong>$ Stored at cycle start</strong></div>
   			</div>	
   			<div class="col-md-2">
   				<div class="cycle-stored-material-head"><strong>$ Received this cycle</strong></div>
   			</div>	
   			<div class="col-md-2">	
   				<div class="cycle-stored-material-head"><strong>Installed this cycle</strong></div>
   			</div>	
   			<div class="col-md-2">	
   				<div class="cycle-stored-material-head"><strong>Stored at cycle end</strong></div>
   			</div>	
   			
   		</div>	
	
<% @material_line_items.each do |material_line_item| %>

   		<div class="row" style="border-bottom: .3px solid gray" id='material-line-item-<%= material_line_item.id %>'>
   			<div class="col-md-2">
   				<div class="stored-material-value"><%= material_line_item.description %></div>
   			</div>	
   			<div class="col-md-2">
   				<div class="stored-material-value"><%= material_line_item.stored_at_cycle_start %></div>
   			</div>	
   			<div class="col-md-2">
   				<div class="stored-material-value"><%= material_line_item.received_this_cycle %></div>
   			</div>	
   			<div class="col-md-2">	
   				<div class="stored-material-value"><%= material_line_item.installed_this_cycle %></div>
   			</div>
   			<div class="col-md-2">	
   				<div class="stored-material-value"><%= material_line_item.stored_at_cycle_end %></div>
   			</div>	
   				   			
   		</div>	
   	<% end %>
	</div>


