<div class="row" style="border-bottom: .5px solid gray" id='job-<%= job.id %>'>
        <div class="col-md-2 jobs-col-descr">
          <div class="job-value"><%= job.description %></div>
        </div>  
        <div class="col-md-1 jobs-col-grid">
          <div class="job-value"><%= job.value %></div>
        </div>  
        <div class="col-md-1 jobs-col-grid">
          <div class="job-value"><%= job.previous_complete %></div>
        </div>  
        <div class="col-md-1 jobs-col-grid">  
          <div class="job-value"><%= job.this_application %></div>
        </div>
        <div class="col-md-1 jobs-col-grid">  
          <div class="job-value"><%= job.completed_to_date_percent %></div>
        </div>  
        <div class="col-md-1 jobs-col-grid">  
          <div class="job-value"><%= job.completed_to_date_value %></div>
        </div>          
        <div class="col-md-1 jobs-col-action">  
          <div class="job-value"><%= link_to 'edit', edit_job_path(job, :scope_cycle_id => @scope_cycle.id), remote: true %></div>
        </div>  
        <div class="col-md-1 jobs-col-action">  
          <div class="job-value"><%= link_to 'delete', job, remote: true, method: :delete, id: job.id  %></div>
        </div>  
      </div>  
